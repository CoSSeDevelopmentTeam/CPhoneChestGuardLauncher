<?xml version="1.0" encoding="UTF-8" ?>
<!--<!DOCTYPE manifest SYSTEM "https://raw.githubusercontent.com/CoSSeDevelopmentTeam/CPhone/master/CPhoneManifest.dtd">-->

<manifest>
    <application>
        <title>ChestGuard</title>
        <description>チェストの管理モードを起動するアプリ</description>
        <author>popkechupki</author>
        <version>1.1.0</version>
        <mcbeversion>1.14</mcbeversion>
        <icon>https://info.comorevi.net/images/favicon/logo_transparent.png</icon>
    </application>

    <permission type = "everyone" />

    <activity>
        <name>MainActivity</name>
        <main>net.comorevi.cpapp.chestguard.MainActivity</main>
    </activity>

    <external-lang>
        <lang region="en">
            <title>ChestGuard</title>
            <description>This is a application to enable/disable editing mode of ChestGuard.</description>
        </lang>
        <lang region="ja">
            <title>チェストガード/ChestGuard</title>
            <description>チェストの管理モードを有効化・無効化するアプリ</description>
        </lang>
    </external-lang>
</manifest>